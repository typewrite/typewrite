{
  "name": "node-blogger",
  "version": "1.0.0",
  "description": "Node blogger CMS tool. Alternative to Wordpress and Ghost",
  "author": "Shalom Sam",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">=8.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/typewrite/typewrite"
  },
  "nyc": {
    "all": true,
    "extension": [
      ".js",
      ".jsx",
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/*.ts",
      "src/**/*.ts",
      "src/**/**/*.ts",
      "src/**/**/**/*.ts",
      "test/*.ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "report-dir": "docs/coverage"
  },
  "scripts": {
    "start": "npx gulp build && node dist/index.js",
    "build": "npx gulp build",
    "dev": "npx nodemon",
    "test": "NODE_ENV=test mocha --opts './tests/mocha.opts'",
    "test:snyk": "snyk test",
    "test:coverage": "NODE_ENV=test nyc mocha --opts './tests/mocha.opts' && nyc report --reporter=text-lcov | coveralls"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^4.1.2",
    "@types/chai-http": "^3.0.4",
    "@types/chalk": "^2.2.0",
    "@types/compression": "0.0.35",
    "@types/cors": "^2.8.3",
    "@types/debug": "0.0.30",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.1",
    "@types/helmet": "0.0.37",
    "@types/jsonwebtoken": "^7.2.5",
    "@types/marked": "^0.3.0",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.6",
    "@types/nodemailer": "^4.3.4",
    "@types/nunjucks": "^3.0.0",
    "@types/uuid": "^3.4.3",
    "@types/winston": "^2.3.8",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "chalk": "^2.3.2",
    "commander": "^2.14.1",
    "coveralls": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^4.0.1",
    "inquirer": "^5.1.0",
    "mocha": "^5.0.1",
    "nodemon": "^1.18.10",
    "npm-check": "^5.9.0",
    "nsp": "^3.2.1",
    "nyc": "^11.4.1",
    "replace-in-file": "^3.1.1",
    "run-sequence": "^2.2.1",
    "shelljs": "^0.8.1",
    "snyk": "^1.130.0",
    "source-map-support": "^0.5.3",
    "ts-node": "^5.0.1",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "helmet": "^3.21.1",
    "https": "^1.0.0",
    "jsonwebtoken": "^8.2.0",
    "marked": "^0.3.18",
    "moment": "^2.21.0",
    "nodemailer": "^6.6.1",
    "nunjucks": "^3.1.2",
    "pg": "^7.4.1",
    "reflect-metadata": "^0.1.12",
    "typeorm": "^0.2.15",
    "routing-controllers": "^0.8.0",
    "uuid": "^3.2.1",
    "winston": "^3.0.0-rc1",
    "winston-daily-rotate-file": "^2.0.0-rc.1"
  }
}
