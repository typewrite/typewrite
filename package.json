        "@types/node": "^8.0.29",
        "@types/nodemailer": "^4.3.1",
        "@types/nunjucks": "^3.0.0",
        "@types/uuid": "^3.4.3",
        "@types/winston": "^2.3.7",
        "chai": "^4.1.2",
        "chai-http": "^3.0.0",
        "chalk": "^2.3.0",
        "commander": "^2.12.2",
        "gulp": "^3.9.1",
        "gulp-clean": "^0.3.2",
        "gulp-sourcemaps": "^2.6.2",
        "gulp-typescript": "^3.2.3",
        "inquirer": "^4.0.1",
