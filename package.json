{
    "name": "node-blogger",
    "version": "1.0.0",
    "main": "dist/index.js",
    "description": "Node blogger CMS tool. Alternative to Wordpress and Ghost",
    "author": "Shalom Sam",
    "license": "MIT",
    "engines": {
        "node": ">=8.9.3"
    },
    "nyc": {
        "all": true,
        "extension": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
        ],
        "include": [
            "src/*.ts",
            "src/**/*.ts",
            "src/**/**/*.ts",
            "src/**/**/**/*.ts",
            "test/*.ts"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "report-dir": "docs/coverage"
    },
    "scripts": {
        "start": "npx gulp build && node dist/index.js",
        "build": "npx gulp build",
        "dev": "npx nodemon",
        "test": "NODE_ENV=test mocha --opts './tests/mocha.opts'",
        "coverage": "NODE_ENV=test nyc mocha --opts './tests/mocha.opts'"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.1",
